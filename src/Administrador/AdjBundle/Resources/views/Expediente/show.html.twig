<h1>Expediente</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Numeroexpedienteky</th>
            <td>{{ entity.numeroexpedienteKy }}</td>
        </tr>
        <tr>
            <th>Titulojuicio</th>
            <td>{{ entity.titulojuicio }}</td>
        </tr>
        <tr>
            <th>Estadoprocesal</th>
            <td>{{ entity.estadoprocesal }}</td>
        </tr>
        <tr>
            <th>Fechainicio</th>
            <td>{{ entity.fechainicio|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Fechatermino</th>
            <td>{{ entity.fechatermino|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Cuantia</th>
            <td>{{ entity.cuantia }}</td>
        </tr>
        <tr>
            <th>Tipoestado</th>
            <td>{{ entity.tipoestado }}</td>
        </tr>
        <tr>
            <th>Tipoestadoborrado</th>
            <td>{{ entity.tipoestadoborrado }}</td>
        </tr>
        <tr>
            <th>Creadoporid</th>
            <td>{{ entity.creadoporId }}</td>
        </tr>
        <tr>
            <th>Creadoel</th>
            <td>{{ entity.creadoel|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Actualizadoporid</th>
            <td>{{ entity.actualizadoporId }}</td>
        </tr>
        <tr>
            <th>Actualizadoel</th>
            <td>{{ entity.actualizadoel|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('expediente') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('expediente_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('expediente_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
